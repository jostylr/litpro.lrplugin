{
  "LRCompilers": [
    {
      "Name": "LiterateProgramming",
      "Extensions": ["md"],
      "DestinationExtension": "lpout",
      "CommandLine": [
        "$(node)",
        "$(plugin)/node_modules/literate-programming/bin/literate-programming.js",
        "-e", "lp.md",
        "$(additional)",
        "$(src_path)",
        "$(dst_dir)"
      ]
    }
  ]
}